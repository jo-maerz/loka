{
    "id": "loka-realm",
    "realm": "loka-realm",
    "enabled": true,
    "clients": [
      {
        "clientId": "loka-client",
        "enabled": true,
        "publicClient": true,
        "redirectUris": [
          "http://localhost:4200/*"
        ],
        "webOrigins": ["+"],
        "protocolMappers": [
          {
            "name": "roles",
            "protocol": "openid-connect",
            "protocolMapper": "oidc-usermodel-realm-role-mapper",
            "config": {
              "claim.name": "roles",
              "jsonType.label": "String",
              "multivalued": "true",
              "userinfo.token.claim": "true",
              "access.token.claim": "true"
            }
          }
        ]
      }
    ],
    "roles": {
      "realm": [
        {
          "name": "ADMIN"
        },
        {
          "name": "VERIFIED"
        }
      ]
    }
  }